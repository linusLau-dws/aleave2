public class UserContent {
    private String u_name;
    private Integer u_userid;
    private String u_nick_name;
    private String u_english_name;
    private String u_chinese_name;
    private String u_staff_number;
    private String u_employment_number;
    private Boolean u_allow3sections;
    private Boolean u_allowhalfday;
    private Boolean u_allowhourly;
    private Integer count;


    //public WorkerContent(String n, String h, String cd, String b, String l, String c, Bitmap p) {
    public UserContent(String n, Integer uid, String nn, String en, String cn, String sn, String enu, Boolean a3s, Boolean ahd, Boolean ah, Integer x) {
        u_name = n;
        u_userid = uid;
        u_nick_name = nn;
        u_english_name = en;
        u_chinese_name = cn;
        u_staff_number = sn;
        u_employment_number = enu;
        u_allow3sections = a3s;
        u_allowhalfday = ahd;
        u_allowhourly = ah;
        count = x;
    }

    public String getUserName() {
        return u_name;
    }

    public Integer getUserId() {
        return u_userid;
    }

    public String getNickName() {
        return u_nick_name;
    }

    public String getEnglishName() {
        return u_english_name;
    }

    public String getChineseName() {
        return u_chinese_name;
    }

    public String getStaffNumber() {
        return u_staff_number;
    }

    public String getEmploymentNumber() {
        return u_employment_number;
    }

    public Boolean getAllow3Sections() {
        return u_allow3sections;
    }

    public Boolean getAllowHalfDay() {
        return u_allowhalfday;
    }

    public Boolean getAllowHourly() {
        return u_allowhourly;
    }

    public Integer getCount() {
        return count;
    }
}
